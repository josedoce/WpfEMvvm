<Window x:Class="WpfEMvvm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfEMvvm"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
   
    <Grid>
        <StackPanel>
            <Label Content="{Binding VeiculoNome, UpdateSourceTrigger=PropertyChanged}"/>
            
            <Button Width="100" Content="Mudar MVVM" Command="{Binding MudarCommand}"/>
        </StackPanel>
    </Grid>
</Window>
